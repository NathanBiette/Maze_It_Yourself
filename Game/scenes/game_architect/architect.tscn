func _ready():
	# Called every time the node is added to the scene.
	# Initialization here
	pass

func add(d):
	var not_in_list = true
	for i in range(get_item_count()):
		if (d[0]*10+d[1] == get_item_ID(i)):
			not_in_list = false
	if not_in_list:
		if d[1] == 0:
			add_item(\"Room \" +str(d[0])+\" north door\", d[0] * 10 + d[1])
		if d[1] == 1:
			add_item(\"Room \" +str(d[0])+\" west door\", d[0] * 10 + d[1])
		if d[1] == 2:
			add_item(\"Room \" +str(d[0])+\" south door\", d[0] * 10 + d[1])
		if d[1] == 3:
			add_item(\"Room \" +str(d[0])+\" east door\", d[0] * 10 + d[1])
		
"

